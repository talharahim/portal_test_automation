<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite for Nexus Portal"
	configfailurepolicy="skip" thread-count="10" verbose="10">
	<listeners>
		<listener
			class-name="com.NexusPortalAutomation.Utilities.Java.CustTestNGListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test thread-count="5" name="NexusTest">
		<classes>
			<class
				name="com.NexusPortalAutomation.TestCases.Java.TC0029_VerifyAction_Transfers_Scheduled" />

			<class
				name="com.NexusPortalAutomation.TestCases.Java.TC0030_VerifyAction_Transfers_StartService_Scheduled" />

			<class
				name="com.NexusPortalAutomation.TestCases.Java.TC0031_VerifyAction_Transfers_StopService_Scheduled" />

			<class
				name="com.NexusPortalAutomation.TestCases.Java.TC0032_VerifyAction_Transfers_Stack_StopService_Scheduled" />

			<class
				name="com.NexusPortalAutomation.TestCases.Java.TC0033_VerifyAction_Transfers_Stack_StartService_Scheduled" />

			<class
				name="com.NexusPortalAutomation.TestCases.TC0034_VerifyAction_Transfers_Stack_TwoLocations" />

			<class
				name="com.NexusPortalAutomation.TestCases.TC0035_VerifyAction_Transfers_StopService" />
			<class
				name="com.NexusPortalAutomation.TestCases.TC0036_VerifyAction_Transfers_StartService" />>

		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
