<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite for Nexus Portal"
	configfailurepolicy="skip" thread-count="10" verbose="10">
	<listeners>
		<listener
			class-name="com.NexusPortalAutomation.Utilities.Java.CustTestNGListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	 <test thread-count="5" name="NexusTest">
        <packages>
            <package name="com.NexusPortalAutomation.TestCases.Java"/>
        </packages>
    </test>
</suite> <!-- Suite -->
